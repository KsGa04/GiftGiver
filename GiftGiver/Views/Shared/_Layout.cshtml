                    <a class="chat" asp-action="ChatBot">Чат-бот</a>
                </div>
                <div class="v133_731" />
            </div>
        </div>
    </header>
    <main class="fon">
    @RenderBody()
    </main>
    <footer>
        <div></div>
    </footer>
</body>
</html>